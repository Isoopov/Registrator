            System.out.println("Вы нажали на кнопку Войти - а Таня смотрит на это)))");
        });
void initialize() {
        ololo.setOnAction(event -> {
            System.out.println("Вы нажали на кнопку Войти - а Таня смотрит на это)))");
        });
    }
}

